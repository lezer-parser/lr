{
  "compilerOptions": {
    "lib": ["es2017"],
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es2015",
    "module": "es2015",
    "newLine": "lf",
    "preserveConstEnums": true,
    "stripInternal": true,
    "moduleResolution": "node"
  },
  "include": ["src/*.ts"]
}
